  flex-wrap: nowrap;
}

.megamenu__link--linked svg{ 
  margin-top: 3px;
}

.megamenu__container {
  display: flex;
  position: absolute;
  left: 0;
  top: 100%;
  width: 100%;
  background-color: #ffffff;
  border-top: 1px solid #0000000f;
  color: #000000;
  padding: 2rem 8rem;
  box-shadow: 0 8px 4px -4px rgb(186, 186, 186);
