<div id="product-choice" class="product-choise {{section.settings.custom_class}}">
  {% assign product = all_products[section.settings.product_handle] %}
  {% if product %}
    {% for block in section.blocks %}
      <!-- code is here -->
      {% for variant in product.variants %}
        {% if block.settings.variant_id contains variant.id %}
          {% assign v = variant %}
          <!-- variant ID {{v.id}} -->
          {% break %}
        {% endif %}
      {% endfor %}

      <div
        class="single-product-wrapper"
        data.option="{{ v.option1 }}"
        data.option2="{{ v.option2 }}"
        data-v="{{ v.id }}"
      >
        {{ block.settings.title }}
        <!--
          <i class="sub-heading do-add-to-cart">
            {{ block.settings.message }} <br />
            {{ block.settings.fastbar_message }}
          </i>
        -->
        <div class="ribbon">
          <img src="{{ block.settings.image | img_url: '1000x' }}" class="do-add-to-cart">
        </div>
        <div class="price-box tez  {{ block.settings.price_class }}">
          <div class="variant-input">
            <div class="input_lable">
              <label class="variant__button-label">
                {{ block.settings.message }}

                <!-- badge badge_2 <span class="intemittant-badge" style="">+20ct Intermittent Fasting bars</span> -->

                <span class="intemittant-badge" style=""> {{ block.settings.fastbar_message }}</span>
              </label>
            </div>
            <div
              class="price-wrapper"
              style="
                position: relative;
                top: -10px;
              "
            >
              <s
                data-price="{{v.price}}"
                compare-at-price
                style="font-size: 17px;line-height: 1;margin-bottom: -5px;display: block;margin-top: 5px; color:red; "
              >
                {% capture dPrice %}
                  {{ v.price }}
                {% endcapture %}
                {% if dPrice contains '170'
                  or dPrice contains '175'
                  or dPrice contains '179'
                  or dPrice contains '185'
                  or dPrice contains '187'
                %}
                  $199.00
                {% elsif dPrice contains '340' or dPrice contains '350' %}
                  $398.00
                {% elsif dPrice contains '510' %}
                  $597.00
                {% endif %}
              </s>
              <div class="t-price"  single-price>
                {{ v.price | money }}
              </div>

              <small style="line-height: 1;font-size: 11px;">AS LOW AS </small>
              <div
                class="t-price"
                style="          font-size: 16px;"
                four-month-price
              > 
                <!-- <s>         $249</s> -->

                {{ v.price | divided_by: 6 | money }} / Month
              </div>
            </div>
          </div>
          <!--
            <p class="main-price {{ block.settings.price_class }}">

              {{v.price | divided_by: 4 | money}}<small style="
              font-size: 14px;
              display: inline;
              font-weight: 100;color:black;

              "> per installment</small>
            </p>
          -->
          {% comment %}
            {% include 'price-shop-pay-new',price: v.price %}
          {% endcomment %}
        </div>

        <!--
          <div class="price-box do-add-to-cart tez 32">
            <p class="main-price {{ block.settings.price_class }}">
           {{v.price | money}}
           {% if block.settings.per_box_price_text %} <small style="
          font-size: 14px;
          display: inline;
          font-weight: 100;
          color:black;
          ">{{block.settings.per_box_price_text}}</small>{% endif %}

            </p>

          </div>
        -->
        {% if block.settings.image2 %}
          <div class="ribbon">
            <img src="{{ block.settings.image2 | img_url: '1000x' }}" class="do-add-to-cart">
          </div>
        {% endif %}

        <!--
          <div class="description">


          </div>
        -->

        <!--
          <div class="sub-desc do-add-to-cart">
            {{ block.settings.subDescription }}
          </div>
        -->

        <div class="add-to-cart-section">
          <div class="choose-flavor">
            <div class="variant-input" data-index="option1">
              {% if v.option2 contains '4 ReSet Bundle' %}
              <span>
                <input 
                  id="variety-{{ v.id }}"
                  type="radio"
                  name="variant-{{ v.id }}-flavor"
                   checked   
                  class="flavor-type"
                  value="Variety" 
                >  
                <label for="variety-{{ v.id }}">Variety </label>
              </span> 
              {% endif  %}
              <span>
                <input 
                  id="gen3-{{ v.id }}"
                  type="radio"
                  name="variant-{{ v.id }}-flavor"
                  {% unless v.option2 contains '4 ReSet Bundle' %}
                  checked
            {%endunless%}
                  class="flavor-type"
                  value="Gen3"
                >  
                <label for="gen3-{{ v.id }}">Gen3 </label>
              </span> 
              <span> 
                <input
                  id="original-flavor-{{ v.id }}"
                  name="variant-{{ v.id }}-flavor"
                  type="radio"
                 
                  class="flavor-type"
                  value="Original"
                > 
                <label for="original-flavor-{{ v.id }}">Original</label>
              </span>   

              {% comment %} <span>
                <input
                  id="new-flavor-{{ v.id }}"
                  type="radio"
                  name="variant-{{ v.id }}-flavor"
                   
                  class="flavor-type"
                  value="New Flavors"
                >
                <label for="new-flavor-{{ v.id }}">Variety 2</label>
              </span>
               {% endcomment %}
            </div>
          </div>
          <div class="flavour-description"></div>
           {% include 'add-bottle-bump' %}
          <a 
            data-variant-id="{{ v.id }}"
            data-option-2="{{ v.option2 }}"
            class="select-product-and-process pointer site-nav__link site-nav__link--underline"
          >
            <span class="btn">Add To Cart</span>
          </a>
          <!-- <div class="button--sub--bold transparent">Free Shipping Included<br></div> -->

          {% include 'low-installment-badge' %}

          {{ block.settings.below_add_to_cart }}
          {% comment %}
            {% include 'price-klarna-new',price: v.price %}
          {% endcomment %}
        </div>
      </div>
    {% endfor %}
  {% endif %}
</div>

