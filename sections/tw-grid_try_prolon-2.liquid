<!-- tw-grid_try_prolon-2 -->
<div id="product-choice" class="product-choise">
  {% for block in section.blocks %}
    <!-- block.variant_id {{block.settings.variant_id}} -->
    {% if block.settings.product_handle and block.settings.product_handle != '' %}
    {% assign pr= all_products[block.settings.product_handle]  %}
    {% else  %}
    {% assign pr= product  %}
    {% endif  %}
    {% for variant in pr.variants %}
      {% if block.settings.variant_id contains variant.id %}
        {% assign v = variant %}
        <!-- variant ID {{v.id}} -->
        {% break %}
      {% endif %}
    {% endfor %}

    <div
      class="single-product-wrapper"
      data.option="{{ v.option1 }}"
      data-v="{{ block.settings.variant_id }}"
    >
      {{ block.settings.title }}
      <i class="sub-heading do-add-to-cart">
        {{ block.settings.message }}
        <br>
        {{ block.settings.fastbar_message }}
      </i>
      <div class="price-box do-add-to-cart tez 32">
        <p class="main-price {{ block.settings.price_class }}">
          <s class="text-gray-500 compare-at-price">
            <!-- {{ block.settings.comp_price }} -->
            {{ v.compare_at_price | money_without_trailing_zeros }}
          </s>
          <!-- {{ block.settings.price }} -->
          {{ v.price | money_without_trailing_zeros }}
        </p>
        <small>
          {{ block.settings.per_box_text | default: 'per box' }}
        </small>
      </div>
      <div class="ribbon">
        <img src="{{ block.settings.image | img_url: '1000x' }}" class="do-add-to-cart">
      </div>

      <div class="description">
        {{ block.settings.description }}
      </div>

      <div class="sub-desc do-add-to-cart">
        {{ block.settings.subDescription }}
      </div>

      <div class="pt-3 add-to-cart-section">
        <add-to-cart
          variant-id="{{v.id}}"  
          product-handle="{{block.settings.product_handle}}"
          data-option-index="{{section.settings.quantity_option_index}}"
          data-flavor-option-index="{{section.settings.option_index}}"
          data-option="{{ v.option2 }}" 
          data-subscription-option-index="{{block.settings.subscription_option_index | default: section.settings.subscription_option_index }}"
        /> 
        <div class="pt-3 add-to-cart-section animate-pulse">
          <div class="choose-flavor">
            <div class="flex justify-between max-w-[240px] mb-3 mx-auto w-full">
              <span class="flex items-center block w-full"
                ><span class="flex items-center justify-center w-5 h-5 mr-2 bg-white border border-transparent rounded-full bg-prolon_secondary"
                  ><span class="w-3 h-3 bg-white rounded-full"></span></span
                ><label class="w-20 h-4 mb-0 bg-gray-400 bg-orange-200"></label></span
              ><span class="flex items-center justify-end block w-full"
                ><span class="flex items-center justify-center w-5 h-5 mr-2 bg-white border border-transparent rounded-full bg-prolon_secondary"
                  ><span class="w-4 h-3 h-4 bg-white rounded-full"></span></span
                ><label class="w-20 h-4 mb-0 bg-gray-400 bg-orange-200"></label
              ></span>
            </div>
          </div>
          <button type="button" class="h-10 mb-3 bg-orange-200 bg-buttonColor w-60"></button>
        </div>
      </div>
    </div>
  {% endfor %}
</div>

