{% schema %}
{
  "name": "Grid Try Prolon 2",
  "settings": [
    {
      "type": "image_picker",
      "id": "desktop_image",
      "label": "Desktop Image"
    },
    {
      "type": "text",
      "id": "option_index",
      "label": "Flavor Option Index"
    },
    {
      "type": "text",
      "id": "quantity_option_index",
      "label": "Quantity Option Index"
    },
    {
      "type": "text",
      "id": "subscription_option_index",
      "label": "Subscription Option Index"
    }
  ],
  "blocks": [
    {
      "type": "card",
      "name": "Card",
      "settings": [
        {
          "type": "text",
          "id": "product_handle",
          "label": "Product Handle",
          "info": "Enter the product handle"
        },
        {
        "type": "text",
        "id": "subscription_option_index",
        "label": "Subscription Option Index"
        },
        {
          "type": "text",
          "id": "variant_id",
          "label": "Variant ID"
        },
        {
          "type": "textarea",
          "id": "title",
          "label": "Title"
        },
        {
          "type": "textarea",
          "id": "fastbar_message",
          "label": "Fastbar Message"
        },
        {
          "type": "textarea",
          "id": "per_box_text",
          "label": "Per Box Text"
        },
        {
          "type": "textarea",
          "id": "message",
          "label": "Message"
        },
        {
          "type": "textarea",
          "id": "description",
          "label": "Description"
        },
        {
          "type": "textarea",
          "id": "subDescription",
          "label": "Sub Description"
        },
        {
          "type": "textarea",
          "id": "price_class",
          "label": "Price Class"
        },
        {
          "type": "text",
          "id": "price",
          "label": "Price"
        },
        {
          "type": "text",
          "id": "comp_price",
          "label": "Price Compare"
        },

        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        }
      ]
    }
  ]
}
{% endschema %}
