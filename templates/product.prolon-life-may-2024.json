    "dk_faq_dnnNc8": {
      "type": "dk-faq",
      "blocks": {
        "questions_block_n8ewQD": {
          "type": "questions_block",
          "settings": {
            "question": "What’s Inside the Box?",
            "headingtext": "What’s Inside the Box?",
            "content": "<div class=\"text-center\">\n  \n  <p class=\" text-left w-full md:w-2/3 m-auto pt-2\" style=\"text-align: center;\">With Prolon, you get everything you need pre-packaged and ready to go. Each kit comes with five boxes which are labeled by day so you know what to eat each day.</p>\n  <div class=\"flex flex-wrap gap-[30px] justify-left md:justify-center md:items-baseline pt-11\">\n    \n      <div class=\"flex justify-center items-stretch md:h-1/2 md:w-[30%] gap-3 modal--item-show\" data-image=\"//prolonlife.com/cdn/shop/files/Soups.png?v=1708641808&amp;width=500\">\n        <div class=\"flex-initial\">\n          \n            <button class=\"open--modal-box relative w-[70px] h-[71px]\">\n              <img src=\"//prolonlife.com/cdn/shop/files/BTN1.png?v=1708629137&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/BTN1.png?v=1708629137&amp;width=70 70w\" width=\"70\" height=\"69\">\n              <img src=\"//prolonlife.com/cdn/shop/files/Property_1_Default.png?v=1708710397&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/Property_1_Default.png?v=1708710397&amp;width=70 70w\" width=\"70\" height=\"69\" class=\"hover-img hidden\">\n            </button>\n          \n        </div>\n        <div class=\"flex-1 text-left\">\n          <h6 class=\"font-semibold text-[#73984A] text-xl tracking-tighter capitalize\">\n            Soups\n          </h6>\n          <div class=\"text-xs md:font-normal text-[#4B5563] md:text-sm tracking-tighter pt-1\">\n            A variety of easy-to-prepare soups made with a proprietary mix of all-natural, plant-based ingredients\n          </div>\n        </div>\n        <div class=\"hidden modal--container z-20 fixed top-0 right-0 bottom-0 left-0 flex flex-col items-center justify-center bg-black bg-opacity-75\" data-image=\"files/Soups.png\">\n          <div class=\"w-fit px-6\">\n            <div class=\"flex justify-between items-center py-2\">\n              <h6 class=\"font-semibold text-white text-[32px] tracking-tighter capitalize\">\n                Soups\n              </h6>\n              <button class=\" w-8 h-8 rounded-full bg-white close--modal text-lg\">x</button>\n            </div>\n            <img src=\"//prolonlife.com/cdn/shop/files/Soups.png?v=1708641808&amp;width=800\" alt=\"Prolon's Mushroom Soup, Menestrone Blend, Minestrone &amp; Quinoa Blend &amp; Tomat Soup Blend shown with their ingredients\" srcset=\"//prolonlife.com/cdn/shop/files/Soups.png?v=1708641808&amp;width=352 352w, //prolonlife.com/cdn/shop/files/Soups.png?v=1708641808&amp;width=800 800w\" width=\"800\" class=\"drawer-item__image\">\n          </div>\n        </div>\n      </div>\n    \n      <div class=\"flex justify-center items-stretch md:h-1/2 md:w-[30%] gap-3 modal--item-show\" data-image=\"//prolonlife.com/cdn/shop/files/Nut_Bars.png?v=1708641808&amp;width=500\">\n        <div class=\"flex-initial\">\n          \n            <button class=\"open--modal-box relative w-[70px] h-[71px]\">\n              <img src=\"//prolonlife.com/cdn/shop/files/BTN2.png?v=1708629137&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/BTN2.png?v=1708629137&amp;width=70 70w\" width=\"70\" height=\"69\">\n              <img src=\"//prolonlife.com/cdn/shop/files/Property_2_Default_1.png?v=1708710397&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/Property_2_Default_1.png?v=1708710397&amp;width=70 70w\" width=\"70\" height=\"69\" class=\"hover-img hidden\">\n            </button>\n          \n        </div>\n        <div class=\"flex-1 text-left\">\n          <h6 class=\"font-semibold text-[#73984A] text-xl tracking-tighter capitalize\">\n            Nut Bars\n          </h6>\n          <div class=\"text-xs md:font-normal text-[#4B5563] md:text-sm tracking-tighter pt-1\">\n            Cold-pressed, delicious nut bars with whole ingredients like almonds, macadamia nuts, and pecans\n          </div>\n        </div>\n        <div class=\"hidden modal--container z-20 fixed top-0 right-0 bottom-0 left-0 flex flex-col items-center justify-center bg-black bg-opacity-75\" data-image=\"files/Nut_Bars.png\">\n          <div class=\"w-fit px-6\">\n            <div class=\"flex justify-between items-center py-2\">\n              <h6 class=\"font-semibold text-white text-[32px] tracking-tighter capitalize\">\n                Nut Bars\n              </h6>\n              <button class=\" w-8 h-8 rounded-full bg-white close--modal text-lg\">x</button>\n            </div>\n            <img src=\"//prolonlife.com/cdn/shop/files/Nut_Bars.png?v=1708641808&amp;width=800\" alt=\"Prolon's Honey &amp; Nut Fast Bars displayed on a table with honey &amp; almonds, pecans and other nuts\" srcset=\"//prolonlife.com/cdn/shop/files/Nut_Bars.png?v=1708641808&amp;width=352 352w, //prolonlife.com/cdn/shop/files/Nut_Bars.png?v=1708641808&amp;width=800 800w\" width=\"800\" class=\"drawer-item__image\">\n          </div>\n        </div>\n      </div>\n    \n      <div class=\"flex justify-center items-stretch md:h-1/2 md:w-[30%] gap-3 modal--item-show\" data-image=\"//prolonlife.com/cdn/shop/files/Snacks.png?v=1708641808&amp;width=500\">\n        <div class=\"flex-initial\">\n          \n            <button class=\"open--modal-box relative w-[70px] h-[71px]\">\n              <img src=\"//prolonlife.com/cdn/shop/files/BTN3.png?v=1708629136&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/BTN3.png?v=1708629136&amp;width=70 70w\" width=\"70\" height=\"69\">\n              <img src=\"//prolonlife.com/cdn/shop/files/Property_3_Default_1.png?v=1708710397&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/Property_3_Default_1.png?v=1708710397&amp;width=70 70w\" width=\"70\" height=\"70\" class=\"hover-img hidden\">\n            </button>\n          \n        </div>\n        <div class=\"flex-1 text-left\">\n          <h6 class=\"font-semibold text-[#73984A] text-xl tracking-tighter capitalize\">\n            Snacks\n          </h6>\n          <div class=\"text-xs md:font-normal text-[#4B5563] md:text-sm tracking-tighter pt-1\">\n            Snacks like olives from the South of Spain where olive consumption is associated with longevity, and kale crackers enriched with plant-based protein.\n          </div>\n        </div>\n        <div class=\"hidden modal--container z-20 fixed top-0 right-0 bottom-0 left-0 flex flex-col items-center justify-center bg-black bg-opacity-75\" data-image=\"files/Snacks.png\">\n          <div class=\"w-fit px-6\">\n            <div class=\"flex justify-between items-center py-2\">\n              <h6 class=\"font-semibold text-white text-[32px] tracking-tighter capitalize\">\n                Snacks\n              </h6>\n              <button class=\" w-8 h-8 rounded-full bg-white close--modal text-lg\">x</button>\n            </div>\n            <img src=\"//prolonlife.com/cdn/shop/files/Snacks.png?v=1708641808&amp;width=800\" alt=\"A healthy &amp; light meal with kale crackers, olives, hibiscus herbal tea, and other snacks \" srcset=\"//prolonlife.com/cdn/shop/files/Snacks.png?v=1708641808&amp;width=352 352w, //prolonlife.com/cdn/shop/files/Snacks.png?v=1708641808&amp;width=800 800w\" width=\"800\" class=\"drawer-item__image\">\n          </div>\n        </div>\n      </div>\n    \n      <div class=\"flex justify-center items-stretch md:h-1/2 md:w-[30%] gap-3 modal--item-show\" data-image=\"//prolonlife.com/cdn/shop/files/Supplements.png?v=1708641808&amp;width=500\">\n        <div class=\"flex-initial\">\n          \n            <button class=\"open--modal-box relative w-[70px] h-[71px]\">\n              <img src=\"//prolonlife.com/cdn/shop/files/BTN4.png?v=1708629136&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/BTN4.png?v=1708629136&amp;width=70 70w\" width=\"70\" height=\"70\">\n              <img src=\"//prolonlife.com/cdn/shop/files/Property_4_Default_1.png?v=1708710397&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/Property_4_Default_1.png?v=1708710397&amp;width=70 70w\" width=\"70\" height=\"70\" class=\"hover-img hidden\">\n            </button>\n          \n        </div>\n        <div class=\"flex-1 text-left\">\n          <h6 class=\"font-semibold text-[#73984A] text-xl tracking-tighter capitalize\">\n            Supplements\n          </h6>\n          <div class=\"text-xs md:font-normal text-[#4B5563] md:text-sm tracking-tighter pt-1\">\n            Dietary supplement packed with multivitamins, amino-acids, minerals and omega-3 fatty acid to provide the body with nourishment\n          </div>\n        </div>\n        <div class=\"hidden modal--container z-20 fixed top-0 right-0 bottom-0 left-0 flex flex-col items-center justify-center bg-black bg-opacity-75\" data-image=\"files/Supplements.png\">\n          <div class=\"w-fit px-6\">\n            <div class=\"flex justify-between items-center py-2\">\n              <h6 class=\"font-semibold text-white text-[32px] tracking-tighter capitalize\">\n                Supplements\n              </h6>\n              <button class=\" w-8 h-8 rounded-full bg-white close--modal text-lg\">x</button>\n            </div>\n            <img src=\"//prolonlife.com/cdn/shop/files/Supplements.png?v=1708641808&amp;width=800\" alt=\"Two women each holding up a single L-Pill capsule from Prolon\" srcset=\"//prolonlife.com/cdn/shop/files/Supplements.png?v=1708641808&amp;width=352 352w, //prolonlife.com/cdn/shop/files/Supplements.png?v=1708641808&amp;width=800 800w\" width=\"800\" class=\"drawer-item__image\">\n          </div>\n        </div>\n      </div>\n    \n      <div class=\"flex justify-center items-stretch md:h-1/2 md:w-[30%] gap-3 modal--item-show\" data-image=\"//prolonlife.com/cdn/shop/files/L-drink.png?v=1708641807&amp;width=500\">\n        <div class=\"flex-initial\">\n          \n            <button class=\"open--modal-box relative w-[70px] h-[71px]\">\n              <img src=\"//prolonlife.com/cdn/shop/files/BTN5.png?v=1708629137&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/BTN5.png?v=1708629137&amp;width=70 70w\" width=\"70\" height=\"69\">\n              <img src=\"//prolonlife.com/cdn/shop/files/Property_5_Default_1.png?v=1708710397&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/Property_5_Default_1.png?v=1708710397&amp;width=70 70w\" width=\"70\" height=\"69\" class=\"hover-img hidden\">\n            </button>\n          \n        </div>\n        <div class=\"flex-1 text-left\">\n          <h6 class=\"font-semibold text-[#73984A] text-xl tracking-tighter capitalize\">\n            L-Drink\n          </h6>\n          <div class=\"text-xs md:font-normal text-[#4B5563] md:text-sm tracking-tighter pt-1\">\n            Fruit-flavored, proprietary glycerol mix designed to fuel and protect muscles throughout the fast\n          </div>\n        </div>\n        <div class=\"hidden modal--container z-20 fixed top-0 right-0 bottom-0 left-0 flex flex-col items-center justify-center bg-black bg-opacity-75\" data-image=\"files/L-drink.png\">\n          <div class=\"w-fit px-6\">\n            <div class=\"flex justify-between items-center py-2\">\n              <h6 class=\"font-semibold text-white text-[32px] tracking-tighter capitalize\">\n                L-Drink\n              </h6>\n              <button class=\" w-8 h-8 rounded-full bg-white close--modal text-lg\">x</button>\n            </div>\n            <img src=\"//prolonlife.com/cdn/shop/files/L-drink.png?v=1708641807&amp;width=800\" alt=\"Two 4 ounce bottles of Prolon's L-Drink\" srcset=\"//prolonlife.com/cdn/shop/files/L-drink.png?v=1708641807&amp;width=352 352w, //prolonlife.com/cdn/shop/files/L-drink.png?v=1708641807&amp;width=800 800w\" width=\"800\" class=\"drawer-item__image\">\n          </div>\n        </div>\n      </div>\n    \n      <div class=\"flex justify-center items-stretch md:h-1/2 md:w-[30%] gap-3 modal--item-show\" data-image=\"//prolonlife.com/cdn/shop/files/Herbal_Teas.png?v=1708641807&amp;width=500\">\n        <div class=\"flex-initial\">\n          \n            <button class=\"open--modal-box relative w-[70px] h-[71px]\">\n              <img src=\"//prolonlife.com/cdn/shop/files/BTN6.png?v=1708629137&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/BTN6.png?v=1708629137&amp;width=70 70w\" width=\"70\" height=\"69\">\n              <img src=\"//prolonlife.com/cdn/shop/files/Property_6_Default_1.png?v=1708710397&amp;width=70\" alt=\"\" srcset=\"//prolonlife.com/cdn/shop/files/Property_6_Default_1.png?v=1708710397&amp;width=70 70w\" width=\"70\" height=\"70\" class=\"hover-img hidden\">\n            </button>\n          \n        </div>\n        <div class=\"flex-1 text-left\">\n          <h6 class=\"font-semibold text-[#73984A] text-xl tracking-tighter capitalize\">\n            Herbal Teas\n          </h6>\n          <div class=\"text-xs md:font-normal text-[#4B5563] md:text-sm tracking-tighter pt-1\">\n            Caffeine-free herbal teas loaded with healthy antioxidants, and will keep you hydrated during your week.\n          </div>\n        </div>\n        <div class=\"hidden modal--container z-20 fixed top-0 right-0 bottom-0 left-0 flex flex-col items-center justify-center bg-black bg-opacity-75\" data-image=\"files/Herbal_Teas.png\">\n          <div class=\"w-fit px-6\">\n            <div class=\"flex justify-between items-center py-2\">\n              <h6 class=\"font-semibold text-white text-[32px] tracking-tighter capitalize\">\n                Herbal Teas\n              </h6>\n              <button class=\" w-8 h-8 rounded-full bg-white close--modal text-lg\">x</button>\n            </div>\n            <img src=\"//prolonlife.com/cdn/shop/files/Herbal_Teas.png?v=1708641807&amp;width=800\" alt=\"One tea bag of Prolon's Hibiscus Tea on top of dried hibiscus leaves\" srcset=\"//prolonlife.com/cdn/shop/files/Herbal_Teas.png?v=1708641807&amp;width=352 352w, //prolonlife.com/cdn/shop/files/Herbal_Teas.png?v=1708641807&amp;width=800 800w\" width=\"800\" class=\"drawer-item__image\">\n          </div>\n        </div>\n      </div>\n    \n  </div>\n\n  <div class=\"hidden fixed top-0 bottom-0 left-0 right-0 w-full h-full bg-black bg-opacity-50 backdrop-blur-3 z-10\"></div>\n  \n</div>",
