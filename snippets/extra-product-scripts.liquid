<style>

  .classic-kalivyo-popup {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100vw;
    height: 100vh;
    display: none;
    align-items: center;
    justify-content: center;
}

.popup-wrapper-bg {
    background: #0000007a;
    width: 100vw;
    position: absolute;
    top: 0px;
    left: 0px;
    height: 100vh;
    z-index: 1;
    backdrop-filter: blur(2.5px);
}

.classic-kalivyo-popup .form-wrapper {
    z-index: 99;
    opacity:0;
    transition:all 1s ease;
}
body.show-audrey-popup{
    overflow:hidden;
}

body.show-audrey-popup .classic-kalivyo-popup{
    display:flex;
} 
svg.klaviyo-header-close {
    margin-left:auto;
    margin-right:0px;
    margin-bottom: 6px;
    display: block;
    cursor: pointer;
}    
body.show-audrey-popup .classic-kalivyo-popup .form-wrapper {
    opacity:1;
}
.klaviyo-form-ThykhT.klaviyo-form.form-version-cid-1>svg {
    display: none;
} 
.klaviyo-form-ThykhT svg {
    width: 40px;
    height: 40px;
} 
</style>
