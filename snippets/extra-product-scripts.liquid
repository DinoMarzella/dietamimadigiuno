        $('.k-l-form').hide();
        $('.audery-form').show();
        $("body").addClass("show-audrey-popup");   
    });   
    $('[href="#kirk-join"]').click(function(e) {
        e.preventDefault();
        $('.k-l-form').hide();
        $('.kirk-form').show();
        $("body").addClass("show-audrey-popup");   
    });    
    $('[href="#cynthia-join"]').click(function(e) {
        e.preventDefault();
        $('.k-l-form').hide();
        $('.cynthia-form').show();
        $("body").addClass("show-audrey-popup");   
    });    
    
    $(".popup-wrapper-bg,.klaviyo-header-close").click(function () {
      $("body.show-audrey-popup").removeClass("show-audrey-popup");
    }); 
  });
</script>
