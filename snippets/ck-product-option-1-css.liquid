      .wps_upsell_bump_checkbox_container input {
          position: absolute;
          opacity: 0;
          cursor: pointer;
          height: 0;
          width: 0;
      }
     /* test 1-css */
      .checkmark {
          position: absolute;
          top: 0;
          margin: 3px;
          left: 0;
          height: 25px;
          width: 25px;
          background-color: #eeeeee;
          animation: shadow-pulse 1.5s infinite;
          border: 2px solid #73994a;
          border-radius: 20px;
      }
  
      .wps_upsell_offer_arrow {
          width: 40px;
          margin-right: 4px;
          transform: scaleX(-1);
          animation: leftright 0.4s infinite ease;
          padding: 0 2px;
          position: relative;
          top: 5px;
      }
  
      .wps_upsell_offer_arrow svg {
          width: 100%;
          height: auto;
          fill: #dd3333;
      }
  
      .wps_upsell_bump_checkbox_container:hover input~.checkmark {
          background-color: #ccc;
      }
  
      .wps_upsell_bump_checkbox_container input:checked~.checkmark {
          background-color: #ffffff;
      }
  
      .checkmark:after {
          content: "";
          position: absolute;
          display: none;
      }
  
      .wps_upsell_bump_checkbox_container input:checked~.checkmark:after {
          display: block;
      }
  
      .wps_upsell_bump_checkbox_container .checkmark:after {
          left: 9px;
          top: 5px;
          width: 5px;
          height: 10px;
          border: solid #333;
          border-width: 0 3px 3px 0;
          -webkit-transform: rotate(45deg);
          -ms-transform: rotate(45deg);
