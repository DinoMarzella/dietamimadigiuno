<script>
  $(".nutrition-facts-btn a").click(function (e) {
  e.preventDefault();

    let current_text= $(this).html();
    let context= this;
    $(this).html('Loading...');
    setTimeout(() => {
      $(".nutrition-facts-btn a").html('Nutrition Facts'); 
    },2000);
    try{ 
    let nutrition_facts_url = $(this).attr("href");
    let content = "";
    $.get(nutrition_facts_url, function(data){

      let mainContent = $(data).find('#MainContent');
      mainContent.find('.question.active').removeClass('active');  
      $(".nutrition-facts-btn a").html('Nutrition Facts');
      content = mainContent.html(); 
      if (content) {  
    $("body")
      .append(
        '<div class="active" id="video-wrap"><span class="video-overlay"></span><div class="video-container nutrition-fact-popup">' +
        content +   
          '</div><button class="close-video"></button></div>'
      ) 
      .addClass("active"); 
  }   
    });
 
 
}
catch(ex){

  $(".nutrition-facts-btn a").html('Nutrition Facts');
}
});

$(document).on("click", ".close-video, .video-overlay", function () {
  $("#video-wrap").remove();
});
</script>
